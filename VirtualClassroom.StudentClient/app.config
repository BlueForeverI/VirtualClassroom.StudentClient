<?xml version="1.0" encoding="utf-8" ?>
<configuration>
    <!--configure connections to service-->
    <system.serviceModel>
        <bindings>
            <wsHttpBinding>
                <binding name="sessionBinding" maxReceivedMessageSize="2147483647" 
                         closeTimeout="00:10:00" sendTimeout="00:10:00"
                         receiveTimeout="00:10:00" openTimeout="00:10:00">
                    <reliableSession enabled="true" />
                    <security mode="None" />
                  <readerQuotas maxDepth="64" maxStringContentLength="2147483647" 
                                maxArrayLength="2147483647" maxBytesPerRead="4096" 
                                maxNameTableCharCount="16384" />
                </binding>
            </wsHttpBinding>
        </bindings>
        <client>
            <endpoint address="http://virtualclassroomservices.apphb.com/Services/StudentService.svc"
                binding="wsHttpBinding" bindingConfiguration="sessionBinding"
                contract="StudentServiceReference.IStudentService" name="WsHttpBinding_IStudentService" />
        </client>
    </system.serviceModel>
</configuration>